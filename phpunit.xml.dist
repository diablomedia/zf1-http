<?xml version="1.0"?>
<phpunit 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  beStrictAboutTestsThatDoNotTestAnything="true" 
  backupGlobals="true" 
  stderr="true" 
  colors="true" 
  xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/10.5/phpunit.xsd" 
  cacheDirectory=".phpunit.cache"
  displayDetailsOnTestsThatTriggerDeprecations="true" 
  displayDetailsOnTestsThatTriggerErrors="true" 
  displayDetailsOnTestsThatTriggerNotices="true" 
  displayDetailsOnTestsThatTriggerWarnings="true" 
>
  <php>
    <const name="TESTS_ZEND_HTTP_CLIENT_BASEURI" value="false"/>
    <const name="TESTS_ZEND_HTTP_CLIENT_HTTP_PROXY" value="false"/>
    <const name="TESTS_ZEND_HTTP_CLIENT_HTTP_PROXY_USER" value=""/>
    <const name="TESTS_ZEND_HTTP_CLIENT_HTTP_PROXY_PASS" value=""/>
    <const name="TESTS_ZEND_HTTP_USERAGENT_WURFL_LIB_DIR" value="false"/>
    <const name="TESTS_ZEND_HTTP_USERAGENT_WURFL_CONFIG_FILE" value="false"/>
    <const name="TESTS_ZEND_HTTP_USERAGENT_DEVICEATLAS_LIB_DIR" value="false"/>
    <const name="TESTS_ZEND_HTTP_USERAGENT_DEVICEATLAS_DATA_FILE" value="false"/>
    <const name="TESTS_ZEND_HTTP_USERAGENT_TERAWURFL_LIB_DIR" value="false"/>
    <const name="TESTS_ZEND_HTTP_ZF1_FULL_SUITE" value="false"/>
  </php>
  <testsuites>
    <testsuite name="default">
      <directory>./tests</directory>
    </testsuite>
  </testsuites>
  <source>
    <include>
      <directory suffix=".php">./src</directory>
    </include>
  </source>
</phpunit>
